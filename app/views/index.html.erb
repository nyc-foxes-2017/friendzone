<div class="links-left">
  <ul class="links-list">
    <li><%= link_to current_user.name, user_path(current_user) %></li>
  </ul>
</div>

<div class="post-form">
  <%= render 'shared/_post.html.erb' %>
</div>

<div class="newsfeed-center">
  <table class = "newsfeed-table">
    <tr>
      <% @posts.each do |post|%>
      <td><%= post.content%></td>
    </tr>
    <tr>
      <td><%= time_ago_in_words(post.created_at)%></td>
    </tr>
    <% end %>
  </table>
</div>


